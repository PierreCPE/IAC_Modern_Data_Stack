# Configuration Azure Entreprise (requis)
# Connection string pour le stockage Azure fourni par l'entreprise
# IMPORTANT : Remplacez par votre vraie connection string et ajoutez .env à .gitignore !
export TF_VAR_azure_connection_string="DefaultEndpointsProtocol=https;AccountName=VOTRE_STORAGE_ACCOUNT;AccountKey=VOTRE_CLE_SECRETE;EndpointSuffix=core.windows.net"

# Configuration Airbyte (optionnel, utilise les valeurs par défaut si non spécifié)
export TF_VAR_workspace_id="5ae6b09b-fdec-41af-aed7-204436cc6af6"
export TF_VAR_airbyte_server_url="http://localhost:8000"

# Configuration GCS (optionnel - décommentez si vous voulez utiliser GCS)
# GCS_BUCKET_NAME="votre-bucket-gcs"
# GCS_SERVICE_ACCOUNT_KEY='{"type":"service_account","project_id":"..."}'

# Configuration Airbyte (par défaut pour OSS local)
AIRBYTE_SERVER_URL="http://localhost:8000"
AIRBYTE_USERNAME="admin.admin@admin.com"
AIRBYTE_PASSWORD="password"

# Sync Airbyte (optionnel)
# AUTO_TRIGGER_SYNC=true
